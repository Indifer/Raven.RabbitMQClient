<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="ravenRabbitMQ" type="Raven.Message.RabbitMQ.Configuration.ClientConfiguration, Raven.Message.RabbitMQ"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <ravenRabbitMQ logType="Raven.Message.RabbitMQTests.Log,Raven.Message.RabbitMQTests">
    <brokers>
      <broker name="localhost" host="127.0.0.1">
        <queues>
          <queue name="conflict" durable="true"></queue>
          <queue name="redeclare" durable="true" redeclareWhenFailed="true"></queue>
        </queues>
      </broker>
    </brokers>
  </ravenRabbitMQ>
</configuration>